#include<iostream>
#include<math.h>

using namespace std;

     int main()
      {
                int a,b,c;
                float s,ar;
                cin>>a;
                cin>>b;
                cin>>c;
                if(a+b>c&&a+c>b&&c+b>a)
                {
                s=(a+b+c)/2;
                ar=sqrt(s*(s-a)*(s-b)*(s-c));
                cout<< "the area is:" << ar << endl;}
                else
                {
                    cout<< " it is not a triangle.";
                    }

                return 0;
                }

